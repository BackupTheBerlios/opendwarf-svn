	<faivariabletabs>
		<tab class="faiVariable" name="Variables" />
	</faivariabletabs>

	<faitemplatetabs>
		<tab class="faiTemplate" name="Templates" />
	</faitemplatetabs>

	<faiprofiletabs>
		<tab class="faiProfile" name="Profiles" />
