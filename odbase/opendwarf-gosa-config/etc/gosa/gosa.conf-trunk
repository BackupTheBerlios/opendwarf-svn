	</faiprofiletabs>

	<faipackagetabs>
		<tab class="faiPackage" name="Packages" />
	</faipackagetabs>

	<logtabs>
			<tab class="logview" name="System logs" />
			<tab class="gosa_logview" name="GOsa logs" />
	</logtabs>
	
	<main default="default"
		list_summary="false"
		displayerrors="false"
		schema_check="true"
		enableCopyPaste="true"
		forceglobals="true"
		forcessl="false"
		ldapstats="false"
		warnssl="true"
		save_filter="true"
		compressed="true"
		uniq_identifier="entryCNS"
		lang=""
		theme="default"
		session_lifetime="7200"
		compile="/var/spool/gosa"
		debuglevel="0"
		smbhash='perl -MCrypt::SmbHash -e "print join(q[:], ntlmgen \$ARGV[0]), $/;"'
		>

		<location name="default"
			hash="crypt"
			dnmode="cn"
			server="ldap://localhost:389"
			people="ou=people"
			groups="ou=groups"
			gidbase="1000"
			uidbase="1000"
			login_attribute="uid"
			timezone="Europe/Berlin"
			strict_units="false"
			krbsasl="false"
			rfc2307bis="false"
			include_personal_title="false"
			strict="yes"
			governmentmode="false"
			SID="0-815-4711"
			RIDBASE="1"
			sambaversion="3"

		
			config="$CONFIG$">

			<referral url="$LDAPURL$"
				admin="$ADMIN$"
				password="$PASSWORD$" />
		</location>
	</main>
</conf>
